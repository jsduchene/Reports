  <DataSources>
    <DataSource Name="OMDB">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=localhost\SQLp01;Initial Catalog=OperationsManager</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>5727497c-ca51-47c8-af52-74f79419f8fd</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="HeartBeatStatus">
      <Query>
        <DataSourceName>OMDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@HealthState">
            <Value>=Parameters!HealthState.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     bme.DisplayName, s.LastModified AS LastModifiedUTC
FROM         State AS s WITH (NOLOCK) INNER JOIN
                      BaseManagedEntity AS bme WITH (NOLOCK) ON s.BaseManagedEntityId = bme.BaseManagedEntityId
WHERE     (s.MonitorId IN
                          (SELECT     MonitorId
                            FROM          Monitor WITH (NOLOCK)
                            WHERE      (MonitorName = 'Microsoft.SystemCenter.HealthService.Heartbeat'))) AND (s.HealthState = @HealthState) AND (bme.IsDeleted = '0')
ORDER BY LastModifiedUTC DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="DisplayName">
          <DataField>DisplayName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LastModifiedUTC">
          <DataField>LastModifiedUTC</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="HealthState">
      <DataType>String</DataType>
      <Prompt>Health State</Prompt>
      <ValidValues>
        <ParameterValues>
          <ParameterValue>
            <Value>1</Value>
            <Label>Healthy</Label>
          </ParameterValue>
          <ParameterValue>
            <Value>3</Value>
            <Label>Not Healthy</Label>
          </ParameterValue>
        </ParameterValues>
      </ValidValues>
    </ReportParameter>
