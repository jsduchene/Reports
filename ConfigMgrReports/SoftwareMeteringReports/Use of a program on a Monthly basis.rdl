    <DataSet Name="total_usage">
      <Query>
        <DataSourceName>CM_XXX</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Application">
            <Value>=Parameters!Application.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
	DATEPART(year,si.IntervalStart) as 'Year',
	DATENAME(month,si.IntervalStart) as 'Month',
	IsNULL(UC.UCount,0) as 'Local Users',
	IsNULL(TSUC.TSUCount,0) as 'TS Users',
	IsNULL(AUC.AllUCount,0) as 'Total Unique Users'
from v_SummarizationInterval si WITH(NOLOCK)
	LEFT JOIN
	(
	  SELECT TimeKey, count(Distinct MeteredUserID) As 'TSUCount'
	  FROM v_MonthlyUsageSummary WITH(NOLOCK)
	  INNER JOIN v_MeteredFiles WITH(NOLOCK) ON v_MeteredFiles.MeteredFileID = v_MonthlyUsageSummary.FileID
	  INNER JOIN v_MeteredProductRule VMPR WITH(NOLOCK) ON VMPR.RuleID = v_MeteredFiles.RuleID
	  where v_MonthlyUsageSummary.TSUsageCount &gt; 0  
	  and TimeKey &gt;= @StartDate
	  and TimeKey &lt;= @EndDate
	   AND VMPR.FileName = @Application
	  GROUP BY VMPR.ProductName, TimeKey
	) as TSUC on si.TimeKey=TSUC.TimeKey
	LEFT JOIN 
	(
SELECT  TimeKey, count(Distinct MeteredUserID) As 'UCount'
  FROM v_MonthlyUsageSummary WITH(NOLOCK)
  INNER JOIN v_MeteredFiles WITH(NOLOCK) ON v_MeteredFiles.MeteredFileID = [v_MonthlyUsageSummary].FileID
  INNER JOIN v_MeteredProductRule VMPR WITH(NOLOCK) ON VMPR.RuleID = v_MeteredFiles.RuleID
  where v_MonthlyUsageSummary.TSUsageCount = 0  
  and TimeKey &gt;= @StartDate
  and TimeKey &lt;= @EndDate
  AND VMPR.FileName = @Application
  GROUP BY VMPR.ProductName, TimeKey
  ) as UC on si.TimeKey=UC.TimeKey 
left join
(
SELECT TimeKey, count(Distinct MeteredUserID) As 'AllUCount'
  FROM v_MonthlyUsageSummary WITH(NOLOCK)
  INNER JOIN v_MeteredFiles WITH(NOLOCK) ON v_MeteredFiles.MeteredFileID = [v_MonthlyUsageSummary].FileID
  INNER JOIN v_MeteredProductRule VMPR WITH(NOLOCK) ON VMPR.RuleID = v_MeteredFiles.RuleID
  where TimeKey &gt;= @StartDate
  and TimeKey &lt;= @EndDate
  AND VMPR.FileName = @Application
  GROUP BY VMPR.ProductName, TimeKey
) as AUC on si.TimeKey=AUC.TimeKey 
where si.TimeKey &gt;= @StartDate 
and si.TimeKey &lt;= @EndDate
order by Year(si.IntervalStart), Month(si.IntervalStart)</CommandText>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Local_Users">
          <DataField>Local Users</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TS_Users">
          <DataField>TS Users</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total_Unique_Users">
          <DataField>Total Unique Users</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>CM_XXX</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Application">
            <Value>=Parameters!Application.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select TOP 10
	u.FullName as 'User',
	sum(ms.UsageTime) as 'Duration',
	case
		when len(CAST(sum(ms.UsageTime)/3600 as varchar(10))) = 1 then '0' + CAST(sum(ms.UsageTime)/3600 as varchar(10))
		else CAST(sum(ms.UsageTime)/3600 as varchar(10))
	end + ':' +
	case
		when len(CAST(((sum(ms.UsageTime) % 3600) / 60) as varchar(10))) = 1 then '0' + CAST(((sum(ms.UsageTime) % 3600) / 60) as varchar(10))
		else CAST(((sum(ms.UsageTime) % 3600) / 60) as varchar(10))
	end + ':' +
	case
		when len(CAST(((sum(ms.UsageTime) % 3600) % 60) as varchar(10))) = 1 then '0' + CAST(((sum(ms.UsageTime) % 3600) % 60) as varchar(10))
		else CAST(((sum(ms.UsageTime) % 3600) % 60) as varchar(10))
	end as 'HHmmSS'
FROM v_MonthlyUsageSummary ms WITH(NOLOCK)
	INNER JOIN v_MeteredUser u  WITH(NOLOCK) on ms.MeteredUserID = u.MeteredUserID
	INNER JOIN v_MeteredFiles f  WITH(NOLOCK) on ms.FileID = f.MeteredFileID
where f.FileName like @Application
and ms.TimeKey &gt;= @StartDate
and ms.TimeKey &lt;= @EndDate
group by u.FullName
ORDER BY 'Duration' DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="User">
          <DataField>User</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Duration">
          <DataField>Duration</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="HHmmSS">
          <DataField>HHmmSS</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
